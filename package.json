{
  "name": "nti-lib-whiteboardjs",
  "version": "1.3.1",
  "author": "nextthought",
  "license": "UNLICENSED",
  "description": "NextThought Whiteboard Library (Rendering, Interaction, Serialization)",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=5.0.0",
    "npm": ">=3.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NextThought/nti.lib.whiteboardjs.git"
  },
  "scripts": {
